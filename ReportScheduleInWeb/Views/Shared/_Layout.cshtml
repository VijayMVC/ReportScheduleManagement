                    <div class="modal-dialog modal-dialog-centered">
                        <div class="modal-content bg-dark" style="width:100%">
                            <div class="modal-header">
                                <h4 class="modal-title text-white">Изменить пароль</h4>
                                <button type="button" class="close text-white" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                            </div>
                            <div class="modal-body">
                                <form id="form2" style="width:auto">
                                    @Html.HiddenFor(m => m.user_id, new { @id = "UseChangePasswordId" })
                                    <div class="form-group" id="change_old_password">
                                        @Html.EditorFor(m => m.user_password_old, new { htmlAttributes = new { @class = "form-control password", id = "UseOldPassword", placeholder = "Введите старый пароль", @required = true } })
                                        <div class="invalid-feedback field-validation-error">
                                            Введите старый пароль
                                        </div>
                                    </div>
                                    <div class="form-group" id="change_password">
                                        @Html.EditorFor(m => m.user_password, new { htmlAttributes = new { @class = "form-control", id = "UseChangePassword", placeholder = "Введите пароль", @required = true } })
                                        <div class="invalid-feedback field-validation-error">
                                            Введите пароль
                                        </div>
                                    </div>
                                    <div class="form-group" id="change_confirm_password">
                                        @Html.EditorFor(m => m.user_password_confirm, new { htmlAttributes = new { @class = "form-control", id = "UseChangeConfirmPassword", placeholder = "Повторите пароль", @required = true } })
                                        <div class="invalid-feedback field-validation-error">
                                            Пароли не совпадают
                                        </div>
                                    </div>
                                    <div class="form-group" id="error_message">
                                        <div class="field-validation-error">
                                            <span class="oi oi-ban"></span>
                                        </div>
                                    </div>
                                </form>
                            </div>

                            <div class="modal-footer">
                                <div class="form-group">
                                    <button class="btn btn-danger" data-dismiss="modal">Отмена</button>
                                    <input class="btn btn-primary btn-success" type="submit" value="Изменить" id="SaveChangePassword" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </main>
        </div>
    </div>

    @RenderSection("scripts", required: false)
    <script>
        function ChangePassword(UserId) {
	        var url = mainURL + "/User/GetUserById?UserId=" + UserId;
	        $("#form2")[0].reset();
	        $("#MyModalChangePassword").modal();
	        $.ajax({
		        type: "GET",
		        url: url,
		        success: function (data) {
			        var obj = JSON.parse(data);
			        $("#UseChangePasswordId").val(obj.user_id);
		        }
	        })
        }

        $("#SaveChangePassword").click(function () {

            var form = document.getElementById('form2');
            var isValidForm = form.checkValidity();

	        var data = $("#SubmitForm2").serialize();

	        var pass = $("#UseChangePassword").val();
	        var oldpass = $("#UseOldPassword").val();
	        var confpass = $("#UseChangeConfirmPassword").val();

	        if (oldpass.trim() == "") {
		        return false;
	        }

	        if (pass.trim() == "") {
		        return false;
	        }

	        if (pass.trim() != confpass.trim()) {
		        return false;
	        }

	        $.ajax({
		        type: "Post",
		        url: mainURL + "/User/ChangePassword",
		        data: data,
		        success: function (result) {
			        if (result == 0) {
				        return false;
			        }
			        alert("Пароль изменен!");
			        $("#MyModalChangePassword").modal("hide");
		        }
	        })
        })

